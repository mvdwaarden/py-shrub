#################################################################
#    Data properties
#################################################################

###  https://example.org/arch#actions
arch:actions rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:label "actions" .


###  https://example.org/arch#codeName
arch:codeName rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string ;
              rdfs:comment "Safe identifier for UI/diagrams (A–Z, a–z, 0–9, _ only)" ;
              rdfs:label "code name" .


###  https://example.org/arch#intrusiveness
arch:intrusiveness rdf:type owl:DatatypeProperty ;
                   rdfs:range xsd:string ;
                   rdfs:label "intrusiveness" .


###  https://example.org/arch#narrative
arch:narrative rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string ;
               rdfs:label "narrative" .


###  https://example.org/arch#rationale
arch:rationale rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string ;
               rdfs:label "rationale" .


###  https://example.org/arch#weight
arch:weight rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:integer ;
            rdfs:comment "0 = none (gap), 1 = secondary, 2 = core. Use on mapping individuals." ;
            rdfs:label "weight" .


#################################################################
#    Classes
#################################################################

###  https://example.org/arch#AddedValue
arch:AddedValue rdf:type owl:Class ;
                rdfs:comment "Canonical benefits of IA (Clarity of purpose, Coherence & integration, etc.)." ;
                rdfs:label "AddedValue" .


###  https://example.org/arch#Consequence
arch:Consequence rdf:type owl:Class ;
                 rdfs:comment "Negative outcome if a premise is unmanaged (brittleness, outages, injustice, etc.)." ;
                 rdfs:label "Consequence" .


###  https://example.org/arch#ConsequenceIndicator
arch:ConsequenceIndicator rdf:type owl:Class ;
                          rdfs:subClassOf arch:Indicator ;
                          rdfs:label "ConsequenceIndicator" .


###  https://example.org/arch#Domain
arch:Domain rdf:type owl:Class ;
            rdfs:comment "Context of application of IA (IT, Society, Biology, etc.)." ;
            rdfs:label "Domain" .


###  https://example.org/arch#DrillDownContent
arch:DrillDownContent rdf:type owl:Class ;
                      rdfs:comment "Narrative for a specific Premise × AddedValue (used in collapsible UI)." ;
                      rdfs:label "DrillDownContent" .


###  https://example.org/arch#EmergentPattern
arch:EmergentPattern rdf:type owl:Class ;
                     rdfs:comment "Self-organizing structures/behaviors that arise without central design in a given domain." ;
                     rdfs:label "EmergentPattern" .


###  https://example.org/arch#Framework
arch:Framework rdf:type owl:Class ;
               rdfs:comment "Vehicle that carries principles via FrameworkPieces (e.g., TOGAF, SAFe, ISO 42010, Ostrom)." ;
               rdfs:label "Framework" .


###  https://example.org/arch#FrameworkCoverageMapping
arch:FrameworkCoverageMapping rdf:type owl:Class ;
                              rdfs:comment "Associates a Framework, Premise, and AddedValue; carries coverage weight 0/1/2." ;
                              rdfs:label "FrameworkCoverageMapping" .


###  https://example.org/arch#FrameworkPiece
arch:FrameworkPiece rdf:type owl:Class ;
                    rdfs:comment "Concrete construct/artifact inside a framework (e.g., Capability Map, Inspect & Adapt, Nested Enterprises)." ;
                    rdfs:label "FrameworkPiece" .


###  https://example.org/arch#IAIntervention
arch:IAIntervention rdf:type owl:Class ;
                    rdfs:comment "Minimally intrusive action that tunes EA; may reference FrameworkPieces; may be absent (gap) for a Premise×Framework." ;
                    rdfs:label "IAIntervention" .


###  https://example.org/arch#Indicator
arch:Indicator rdf:type owl:Class ;
               rdfs:comment "Measurement item (Premise indicators are leading; Consequence indicators are lagging/observables)." ;
               rdfs:label "Indicator" .


###  https://example.org/arch#Premise
arch:Premise rdf:type owl:Class ;
             rdfs:comment "Condition under which intentional architecture becomes relevant (8 canonical items)." ;
             rdfs:label "Premise" .


###  https://example.org/arch#PremiseIndicator
arch:PremiseIndicator rdf:type owl:Class ;
                      rdfs:subClassOf arch:Indicator ;
                      rdfs:label "PremiseIndicator" .


###  https://example.org/arch#PremisePrincipleJustif
arch:PremisePrincipleJustif rdf:type owl:Class ;
                            rdfs:comment "Associates a Premise and a Principle with a textual rationale (why this principle explains the premise)." ;
                            rdfs:label "PremisePrincipleJustification" .


###  https://example.org/arch#Principle
arch:Principle rdf:type owl:Class ;
               rdfs:comment "Cross-domain explanatory mechanism (e.g., CAS, feedback/cybernetics, pattern language, fractals)." ;
               rdfs:label "Principle" .


###  https://example.org/arch#PrincipleEmbodiment
arch:PrincipleEmbodiment rdf:type owl:Class ;
                         rdfs:comment "Associates a Principle with one or more FrameworkPieces (many-to-many embodiment)." ;
                         rdfs:label "PrincipleEmbodiment" .


###  https://example.org/arch#RelevanceMapping
arch:RelevanceMapping rdf:type owl:Class ;
                      rdfs:comment "Associates a Premise with an AddedValue (conceptual relevance; logical weight 0/1/2 lives here if used)." ;
                      rdfs:label "RelevanceMapping" .


